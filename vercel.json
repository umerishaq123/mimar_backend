{
  "version": 2,
  "builds": [
    {
      "src": "app.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/app.js",
      "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"]
    },
    {
      "src": "/",
      "dest": "/app.js",
      "methods": ["GET"]
    }
  ],
  "regions": ["fra1"],
  "env": {
    "NODE_ENV": "production"
  }
}